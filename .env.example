# .env.example

# This file contains example environment variables.
# Copy this file to .env and fill in your own values.

# Database connection string.
# Example for local PostgreSQL:
# POSTGRES_URL="postgresql://YOUR_USERNAME:YOUR_PASSWORD@localhost:5432/YOUR_DATABASE_NAME"
POSTGRES_URL=

# Auth.js configuration
# You can generate a new secret with `openssl rand -base64 32`
NEXTAUTH_URL=http://localhost:3000
AUTH_SECRET=

# GitHub OAuth credentials for authentication.
# Create a new GitHub OAuth App here: https://github.com/settings/developers
# The callback URL should be http://localhost:3000/api/auth/callback/github
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=
